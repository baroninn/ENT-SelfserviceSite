        <tr class="Win10" hidden>
            <td class="lefttdsettings">Automatic acceptance of the pairing and privacy user consent prompts</td>
            <td class="righttdsettings">
                <select name="privacyAutoAcceptPairingAndConsentPrompts" id="privacyAutoAcceptPairingAndConsentPrompts">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.privacyAutoAcceptPairingAndConsentPrompts == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.privacyAutoAcceptPairingAndConsentPrompts == "True")" value="True">Allow</option>
