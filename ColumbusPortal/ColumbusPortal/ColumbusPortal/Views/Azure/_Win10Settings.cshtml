        <tr class="Win10" hidden>
            <td class="lefttdsettings tooltip">Input personalization<i class="material-icons infoicon">info</i></td>
            <td class="righttdsettings">
                <select name="privacyBlockInputPersonalization" id="privacyBlockInputPersonalization">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.privacyBlockInputPersonalization == "False")" value="False">Not Configured</option>
