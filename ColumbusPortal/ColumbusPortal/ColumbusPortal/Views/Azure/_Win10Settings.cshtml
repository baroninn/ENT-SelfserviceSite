                <select name="lockScreenBlockCortana" id="lockScreenBlockCortana">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockCortana == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockCortana == "True")" value="True">Block</option>
                </select>
                <div class="description">
                    <p class="descriptiontext">Block the user from interacting with Cortana through speech while the system is locked.</p>
                </div>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings tooltip">Toast notifications on locked screen<i class="material-icons infoicon">info</i></td>
            <td class="righttdsettings">
                <select name="lockScreenBlockToastNotifications" id="lockScreenBlockToastNotifications">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockToastNotifications == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockToastNotifications == "True")" value="True">Block</option>
                </select>
                <div class="description">
                    <p class="descriptiontext">Block toast notifications above the device lock screen.</p>
                </div>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings tooltip"><span>Screen timeout (mobile only)</span><i class="material-icons infoicon">info</i></td>
            <td class="righttdsettings">
                <input type="number" min="11" max="1800" id="lockScreenTimeoutInSeconds" name="lockScreenTimeoutInSeconds" placeholder="Number (11-1800)" @if (Model.ActionFailed) { @: value="@Model.AzureDeviceConfiguration.lockScreenTimeoutInSeconds"
                                                                                                                                                    } />
                <!--@Html.TextBoxFor(m => m.AzureDeviceConfiguration.lockScreenTimeoutInSeconds, new { type = "number", placeholder = "Number (11-1800)", id = "lockScreenTimeoutInSeconds" })
                @Html.ValidationMessageFor(m => m.AzureDeviceConfiguration.lockScreenTimeoutInSeconds, "", new { @style = "color:red" })-->
                <div class="description">
                    <p class="descriptiontext">Set the duration (in seconds) from the screen locking to the screen turning off for Windows 10 Mobile devices.</p>
                    <p class="descriptiontext">Supported values are 11-1800. (11-1800)</p>
                </div>
            </td>

        </tr>
    </table>
</fieldset>
