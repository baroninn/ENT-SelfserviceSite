            <td class="righttdsettings">
                <select name="lockScreenBlockActionCenterNotifications" id="lockScreenBlockActionCenterNotifications">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockActionCenterNotifications == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.lockScreenBlockActionCenterNotifications == "True")" value="True">Block</option>
