<fieldset class="Win10 subfieldset" hidden>
    <legend class="sublegend">Password (WIN10)</legend>
    <table>
        <tr class="Win10" hidden>
            <td class="lefttdsettings">Password</td>
            <td class="righttdsettings">
                <select name="winpasswordRequired" id="winpasswordRequired">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequired == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequired == "True")" value="True">Require</option>
                </select>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings">Required password type</td>
            <td class="righttdsettings">
                <select class="Win10Password" name="winpasswordRequiredType" id="winpasswordRequiredType">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequiredType == "deviceDefault")" value="deviceDefault">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequiredType == "numeric")" value="numeric">Numeric</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequiredType == "alphanumeric")" value="alphanumeric">Alphanumeric</option>
                </select>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings"><span>Minimum password length</span></td>
            <td class="righttdsettings">
                <input class="Win10Password" type="number" id="winpasswordMinimumLength" name="winpasswordMinimumLength" placeholder="Number (4-16)" @if (Model.ActionFailed) { @: value="@Model.AzureDeviceConfiguration.winpasswordMinimumLength"
                                                                                                                                                                                                                                                                                                                                   } />
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings">Maximum minutes of inactivity until screen locks</td>
            <td class="righttdsettings">
                <select class="Win10Password" name="winpasswordMinutesOfInactivityBeforeScreenTimeout" id="winpasswordMinutesOfInactivityBeforeScreenTimeout">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == null)" value="">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == "1")" value="1">1 Minute</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == "5")" value="5">5 Minutes</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == "15")" value="15">15 Minutes</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == "30")" value="30">30 Minutes</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordMinutesOfInactivityBeforeScreenTimeout == "60")" value="60">1 Hour</option>
                </select>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings"><span>Number of sign-in failures before wiping device</span></td>
            <td class="righttdsettings">
                <input class="Win10Password" type="number" id="winpasswordSignInFailureCountBeforeFactoryReset" name="winpasswordSignInFailureCountBeforeFactoryReset" placeholder="Number (4-11)" @if (Model.ActionFailed) { @: value="@Model.AzureDeviceConfiguration.winpasswordSignInFailureCountBeforeFactoryReset"
                                                                                                                                                                                                                                                                                                                                                               } />
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings"><span>Password expiration (days)</span></td>
            <td class="righttdsettings">
                <input class="Win10Password" type="number" id="winpasswordExpirationDays" name="winpasswordExpirationDays" placeholder="Number (1-255)" @if (Model.ActionFailed) { @: value="@Model.AzureDeviceConfiguration.winpasswordExpirationDays"
                                                                                                                                                                                                                                                                                                                       } />
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings"><span>Prevent reuse of previous passwords</span></td>
            <td class="righttdsettings">
                <input class="Win10Password" type="number" id="winpasswordPreviousPasswordBlockCount" name="winpasswordPreviousPasswordBlockCount" placeholder="Number (1-24)" @if (Model.ActionFailed) { @: value="@Model.AzureDeviceConfiguration.winpasswordPreviousPasswordBlockCount"
                                                                                                                                                                                                                                                                                                                                                                                                                   } />
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings">Require password when device returns from idle state (mobile only)</td>
            <td class="righttdsettings">
                <select class="Win10Password" name="winpasswordRequireWhenResumeFromIdleState" id="winpasswordRequireWhenResumeFromIdleState">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequireWhenResumeFromIdleState == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordRequireWhenResumeFromIdleState == "True")" value="True">Require</option>
                </select>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings tooltip">Simple Passwords<i class="material-icons infoicon">info</i></td>
            <td class="righttdsettings">
                <select class="Win10Password" name="winpasswordBlockSimple" id="winpasswordBlockSimple">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordBlockSimple == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.winpasswordBlockSimple == "True")" value="True">Block</option>
                </select>
                <div class="description">
                    <p class="descriptiontext">Specify whether PINs or passwords such as "1111" or "1234" are allowed.
                    <p class="descriptiontext">
                        For Windows 10 desktops, it also controls the use of picture passwords.
                    </p>
                </div>
            </td>
        </tr>
        <tr class="Win10" hidden>
            <td class="lefttdsettings tooltip">Encryption<i class="material-icons infoicon">info</i></td>
            <td class="righttdsettings">
                <select class="Win10Password" name="storageRequireMobileDeviceEncryption" id="storageRequireMobileDeviceEncryption">
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.storageRequireMobileDeviceEncryption == "False")" value="False">Not Configured</option>
                    <option selected="@(Model.ActionFailed && Model.AzureDeviceConfiguration.storageRequireMobileDeviceEncryption == "True")" value="True">Require</option>
                </select>
                <div class="description">
                    <p class="descriptiontext">
                        Require encryption on device. Not all devices support encryption.
                        <br />
                    <p class="descriptiontext">You must also:
                    <p class="descriptiontext">1. Enable 'Require password'
                    <p class="descriptiontext">2. Specify 'Password quality' of 'At least numeric'
                    <p class="descriptiontext">3. Set 'Minimum password length' of at least 4, to correctly report compliance for this setting.
                </div>
            </td>
        </tr>
    </table>
</fieldset>
<fieldset class="Win10 subfieldset" hidden>
    <legend class="sublegend">Personalization (WIN10)</legend>
    <table>
        <tr class="Win10" hidden>
