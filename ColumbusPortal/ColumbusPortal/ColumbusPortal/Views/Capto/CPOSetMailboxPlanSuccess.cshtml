@model ColumbusPortal.Models.CaptoModel
@{
    ViewBag.Title = "Capto > Mail > Set MailboxPlan > Success";
}
<h1 class="title">@ViewBag.Title</h1>
<p>The mailbox plan has been set</p>

<fieldset>
    <legend>Set MailboxPlan</legend>
    <table>
        <tr>
            <td class="lefttd">Organization</td>
            <td class="righttd">@Model.CPOMailboxPlan.Organization</td>
        </tr>
        <tr>
            <td class="lefttd">Who?</td>
            <td class="righttd">@Model.CPOMailboxPlan.UserPrincipalName</td>
        </tr>
        <tr>
            <td class="lefttd">Mailbox Plan</td>
            <td class="righttd">@Model.CPOMailboxPlan.MailboxPlan</td>
        </tr>
    </table>
</fieldset>